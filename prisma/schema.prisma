generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Bicycle {
  id         String   @id @default(uuid())
  color      String
  gears      Int
  brand      String
  model      String
  price      Float
  created_at DateTime @default(now())
  uptated_at DateTime @default(now())
  sold       Boolean @default(false)

  @@map("bicycle")
}
